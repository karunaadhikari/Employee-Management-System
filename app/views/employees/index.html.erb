<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Position</th>
      <th scope="col">Gender</th>
      <th scope="col">Join Date</th>
      <th scope="col">Status</th>
      <th scope="col">Email</th>
    </tr>
  </thead>

  <tbody>
    <% @employees.each do |employees| %>
      <tr>
        <td scope="col"><%= employees.fname %></td>
        <td scope="col"><%= employees.lname %> </td>
        <td scope="col"><%= employees.position %></td>
        <td scope="col"><%= employees.gender %></td>
        <td scope="col"><%= employees.join_date %> </td>
        <td scope="col"><%= employees.status %></td>
        <td scope="col"><%= employees.email %> </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @employees %>
